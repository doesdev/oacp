{
  "name": "oacp",
  "version": "1.1.3",
  "description": "Of A Certain Pattern - A framework of sorts",
  "readme": "https://github.com/musocrat/oacp/blob/master/README.md",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musocrat/oacp.git"
  },
  "author": "Andrew Carpenter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/musocrat/oacp/issues"
  },
  "homepage": "https://github.com/musocrat/oacp#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "pg": "^6.1.0"
  },
  "devDependencies": {
    "mocha": "^3.1.0"
  },
  "standard": {
    "global": [
      "describe",
      "it",
      "beforeEach"
    ]
  },
  "oacp": {
    "env": {
      "port": 8080,
      "cors": {
        "whitelist": [
          "*"
        ],
        "preflight": true
      }
    },
    "namespace": "oacp",
    "access": "lib/access.js",
    "jwt": {
      "issuer": "oacp"
    }
  }
}
